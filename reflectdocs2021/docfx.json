{
	"metadata": [
		{
			"src": [
			 {
				 "src": "../../../",
				 "files": [
					 "/Users/corinne.goss/Documents/GitHub/industrial1/Projects/Reflect/**/**.cs",
 					 "/Users/corinne.goss/Documents/GitHub/industrial1/Projects/Reflect/**/**.csproj",
					 "/Users/corinne.goss/Documents/GitHub/industrial1/Projects/CommonUI/**/**.cs",
					 "/Users/corinne.goss/Documents/GitHub/industrial1/Projects/CommonUI/**/**.csproj"
				 ],
				 "exclude": [
					 "**/bin/**",
					 "**/obj/**",
					 "_site/**",
				 ]
				}
			],
			    "dest": "api"
    },
    {
      "src": "**/**.cs",
      "dest": "api-vb"
    }
	],
	"build": {
		"content": [
			{
				"files": [
					"manual/**.md",
					"manual/toc.yml",
					"manual/**/**.md",
					"toc.yml",
					"*.md",
					"api/**.yml",
					"api/index.md",
					"viewerapi/**.md",
					"viewerapi/toc.yml",
				],
				"exclude": [
					"obj/**",
					"_site/**"
				]
			}
		],
		"resource": [
			{
				"files": [
					"images/**",
					"manual/Images/**",
					"logo.svg"
				],
				"exclude": [
					"obj/**",
					"_site/**"
				]
			}
		],
		"overwrite": [
			{
				"files": [
					"apidoc/**.md"
				],
				"exclude": [
					"obj/**",
					"_site/**",
				]
			}
		],
		"template": [
			"./_exported_templates/packages/"
		],
		"postProcessors": [],
		"noLangKeyword": false,
		"keepFileLink": false,
		"dest": "_site",
        "xrefService": [
           "https://xref.docs.microsoft.com/query?uid={uid}"
         ],
		"globalMetadata": {
			"_appTitle": "Unity Reflect",
            "_packageVersion": "2.0",
			"_enableSearch": true,
			"_appLogoPath": "logo.svg",
			"_disableToc": false,
			"enableTocForManual": true
		},
        "globalMetadataFiles": ["projectMetadata.json"]
	}
}
